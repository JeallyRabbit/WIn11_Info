<Window x:Class="WIn11_Info.adminPasswordForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WIn11_Info"
        ResizeMode="NoResize"
        Title="adminPassword" Height="120" Width="234">
    <Grid Margin="0,0,0,0">
        <Label Content="Password:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="87"/>
        <PasswordBox x:Name="passwdBoxPassword" Margin="102,14,12,0"  VerticalAlignment="Top" KeyDown="txtBoxPassword_KeyDown"/>
        <Button x:Name="btnCredentialsOk" Content="OK" Margin="42,45,122,18"  Click="btnCredentialsOK_Click" RenderTransformOrigin="0.5,0.5"/>


        <Button x:Name="btnCredentialsCancel" Content="Cancel" Margin="122,45,42,18" Click="btnCredentialsCancel_Click" RenderTransformOrigin="0.5,0.5">
            
        </Button>


    </Grid>
</Window>
